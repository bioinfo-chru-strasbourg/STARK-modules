DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_PARAM_SUBFOLDER_SERVICE_EDITH=${DOCKER_STARK_MODULE_NAME}/${DOCKER_STARK_MODULE_SUBMODULE_NAME}/dashboard
DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_PARAM_FOLDER_INDEXES=${DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_PARAM_FOLDER_SERVICES}/${DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_PARAM_SUBFOLDER_SERVICE_EDITH}

