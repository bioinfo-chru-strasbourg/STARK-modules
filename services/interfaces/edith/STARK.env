DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE_REPOSITORY=${DOCKER_STARK_SERVICE_IMAGE_REGISTRY}
DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE_NAME=${DOCKER_STARK_MODULE_SUBMODULE_PREFIX}-service-${DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_NAME}
DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE_TAG=${DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_RELEASE}
DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE_CONTEXT=./${DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_NAME}/dockerfile
DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE_CONTEXT_DOCKERFILE=Dockerfile
DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE=${DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE_REPOSITORY}${DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE_NAME}:${DOCKER_STARK_MODULE_SUBMODULE_SERVICE_DASHBOARD_IMAGE_TAG}


